{
  "cmake.configureOnOpen": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "explorer.confirmDelete": false,
  "editor.fontSize": 16,
  "workbench.iconTheme": "catppuccin-latte",
  "workbench.colorTheme": "Tokyo Night Frameless",
  "git.autofetch": true,
  "files.autoSave": "afterDelay",
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "glassit.alpha": 255,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false
  },
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "rapidapi.terminalLink.enabled": false,
  "git.ignoreRebaseWarning": true,
  "tabnine.experimentalAutoImports": true,
  "GitLive.Special branches": "main|master|trunk|dev|develop|qa|test|release[-/].*",
  "GitLive.Issue tracker integration": "Disabled",
  "leetcode.workspaceFolder": "c:\\Users\\Dozzer\\OneDrive - nsec.ac.in\\BIN\\CPP\\codehelp",
  "leetcode.defaultLanguage": "cpp",
  "leetcode.showDescription": "In File Comment",
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.hint.commandShortcut": false,
  "leetcode.hint.commentDescription": false,
  "editor.fontFamily": "'Hack Nerd Font',Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.mouseWheelZoom": true,
  "editor.lineHeight": 25,
  "editor.wordWrap": "on",
  "editor.cursorBlinking": "smooth",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "notebook.formatOnSave.enabled": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.lineHeight": 1.3,
  "files.autoSaveDelay": 1500,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.cursorSmoothCaretAnimation": "on",
  "workbench.layoutControl.enabled": false,
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "redhat.telemetry.enabled": false,
  "material-ui-snippets.showNotesOnStartup": false,
  "security.workspace.trust.untrustedFiles": "open",
  "window.zoomLevel": 2,
  "editor.linkedEditing": true,
  "diffEditor.hideUnchangedRegions.enabled": true

